server.port=8081

spring.datasource.username=root
spring.datasource.password=

spring.datasource.url=jdbc:mysql://[ip]:3306/faas?useSSL=false&useUnicode=true&characterEncoding=utf-8&serverTimezone=UTC&allowPublicKeyRetrieval=true
spring.datasource.driver-class-name=com.mysql.cj.jdbc.Driver
mybatis.type-aliases-package=com.simplefass.registry.pojo
mybatis.mapper-locations=classpath:mybatis/mapper/*.xml

spring.servlet.multipart.max-file-size=5MB
spring.servlet.multipart.max-request-size=100MB
